{
  "name": "slang_exercises",
  "version": "1.0.0",
  "description": "Frond End web app for Slang Spelling Exercises",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.production.config.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production webpack-dev-server --config webpack.production.config.js --progress --profile --colors",
    "ava": "node ./prepare-test.js && tsc --jsx react --inlineSourceMap && ava testJs/**/*.spec.js",
    "test": "cross-env NODE_ENV=test nyc --check-coverage --lines 80  npm run ava",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "dev": "cross-env NODE_ENV=development webpack-dashboard -- webpack-dev-server --progress --profile --colors"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Carlos Urango",
  "ava": {
    "require": [
      "./setup-babel-require.js"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".js"
    ],
    "exclude": [
      "testJs/**/*.spec.js",
      "testJs/**/*.spec.jsx",
      "testJs/**/*.mock.js",
      "testJs/**/*.mock.jsx",
      "setup-babel-require.js",
      "prepare-test.js"
    ],
    "include ": [
      "testJs/**/*.jsx",
      "testJs/**/*.js"
    ]
  },
  "devDependencies": {
    "@types/react": "^0.14.36",
    "@types/react-dom": "^0.14.17",
    "ava": "^0.15.2",
    "awesome-typescript-loader": "^3.0.8",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "codecov": "^1.0.1",
    "enzyme": "^2.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "nyc": "^7.0.0-alpha.5",
    "react-hot-loader": "^3.0.0-beta.6",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "@types/node": "^8.0.31",
    "@types/react-router": "^3.0.6",
    "autoprefixer": "^6.7.6",
    "ava-webpack": "^1.1.2",
    "axios": "^0.16.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "cheerio": "^0.22.0",
    "cpr": "^2.0.2",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "halogen": "^0.2.0",
    "html-webpack-plugin": "^2.28.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mobx": "^2.5.1",
    "mobx-react": "^3.5.6",
    "mobx-react-devtools": "^4.2.6",
    "mobx-remotedev": "^0.2.6",
    "nock": "^9.0.22",
    "postcss-loader": "^1.3.3",
    "postcss-scss": "^0.4.1",
    "precss": "^1.4.0",
    "prop-types": "^15.6.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^3.0.2",
    "react-speech": "^0.3.2",
    "react-transition-group": "1.x",
    "rxjs": "^5.2.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack-cleanup-plugin": "^0.5.1"
  }
}
